operand_1=int(input("Enter operand 1 : "))
operand_2=int(input("Enter operand 2: "))
operator=input("Enter operator (+,-,*,/) : ")

if operator=='+':
    print(operand_1 + operand_2)
elif operator=='-':
    print(operand_1 - operand_2)
elif operator=='*':
    print(operand_1 * operand_2)
elif operator=='/':
    print(operand_1 / operand_2)

else:
    print('None')

print ('Thank you !')